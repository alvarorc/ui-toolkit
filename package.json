{
  "name": "suite-ui-toolkit",
  "version": "0.1.0",
  "description": "UI toolkit made for the FOO suite apps",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.corp.globant.com/alvaro-rago/ui-toolkit.git"
  },
  "author": "Alvaro Rago <alvaro.rago@globant.com>",
  "bugs": {
    "url": "https://github.corp.globant.com/alvaro-rago/ui-toolkit/issues"
  },
  "homepage": "https://github.corp.globant.com/alvaro-rago/ui-toolkit#readme",
  "license": "MIT",
  "engines": {
    "npm": ">=6"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "lodash": "4.17.4",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@storybook/addon-notes": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "autodll-webpack-plugin": "^0.2.1",
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "4.17.4",
    "lodash.defaults": "^4.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier-eslint": "8.2.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.1.1",
    "regenerator-runtime": "0.11.0",
    "rimraf": "^2.6.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "precompile": "rimraf lib",
    "compile":
      "BABEL_ENV=production ./node_modules/.bin/babel src/components --out-dir lib --source-maps --ignore *spec.js,*test.js",
    "prepublish": "npm run compile"
  },
  "dll": {
    "entry": {
      "vendor": ["react", "react-dom", "lodash"]
    }
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": ["web.js", "js", "json", "web.jsx", "jsx", "node"]
  },
  "dependencies": {
    "react-grid-system": "^3.1.0"
  }
}
